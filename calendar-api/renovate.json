{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>henrist/renovate-config:default"],
  "automerge": true,
  "automergeType": "branch",
  "packageRules": [
    {
      "matchPackageNames": ["elysia", "@elysiajs/cors", "@elysiajs/node"],
      "groupName": "elysia"
    },
    {
      "matchPackageNames": ["@googleapis/sheets", "google-auth-library"],
      "groupName": "googleapis"
    }
  ]
}
