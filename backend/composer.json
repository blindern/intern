{
	"name": "blindern/intern",
	"description": "Blindern Studenterhjem Intern",
	"keywords": ["Blindern Studenterhjem", "Laravel"],
	"license": "MIT",
	"type": "project",
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:henrist/openldapauth.git"
		}
	],
	"require": {
		"php": "^8.0.2",
		"google/apiclient": "2.12.6",
		"guzzlehttp/guzzle": "7.4.5",
		"jenssegers/mongodb": "3.9.1",
		"johngrogg/ics-parser": "3.2.0",
		"laravel/framework": "9.22.1",
		"laravel/sanctum": "3.0.0",
		"laravel/tinker": "2.7.2",
		"nategood/httpful": "0.3.2",
		"nesbot/carbon": "2.60.0",
		"onelogin/php-saml": "4.1.0",
		"spatie/icalendar-generator": "^2.3"
	},
	"require-dev": {
		"fakerphp/faker": "1.20.0",
		"laravel/sail": "1.15.1",
		"mockery/mockery": "1.5.0",
		"nunomaduro/collision": "6.2.1",
		"phpunit/phpunit": "9.6.13",
		"spatie/laravel-ignition": "1.3.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/",
			"Blindern\\Intern\\": "app/src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"scripts": {
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi"
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		]
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
